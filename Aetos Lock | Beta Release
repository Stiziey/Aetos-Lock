do getgenv().Sky=true;getgenv().SkyAmount=90;game:GetService("RunService").Heartbeat:Connect(function() if getgenv().Sky then local hrp=game.Players.LocalPlayer.Character.HumanoidRootPart;local originalVelocity=hrp.Velocity;hrp.Velocity=Vector3.new(0,getgenv().SkyAmount,0);game:GetService("RunService").RenderStepped:Wait();hrp.Velocity=originalVelocity;end end);local OrionLib=loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))();local Window=OrionLib:MakeWindow({Name="Aetos Lock | Beta Release",HidePremium=false,SaveConfig=true,ConfigFolder="AimbotConfig",IntroEnabled=false});local aimbotEnabled=false;local targetingPlayer=nil;local targetingBodyPart=nil;local holdingRMB=false;local aimFOV=150;local predictionX=0.05;local predictionY=0.05;local smoothness=10;local lockOnEveryone=false;local fovCircle=Drawing.new("Circle");fovCircle.Thickness=2;fovCircle.NumSides=100;fovCircle.Radius=aimFOV;fovCircle.Color=Color3.fromRGB(255,255,255);fovCircle.Visible=false;fovCircle.Filled=false;game:GetService("RunService").RenderStepped:Connect(function() if fovCircle.Visible then local camera=workspace.CurrentCamera;local viewportSize=camera.ViewportSize;fovCircle.Position=Vector2.new(viewportSize.X/2 ,viewportSize.Y/2 );end end);local timerStart=tick();local timerLabel;local function updateTimer() local elapsedTime=math.floor(tick() -timerStart );local minutes=math.floor(elapsedTime/60 );local seconds=elapsedTime%60 ;if timerLabel then timerLabel:Set("Injection Time: "   .. string.format("%02d:%02d",minutes,seconds) );end end game:GetService("RunService").RenderStepped:Connect(updateTimer);local function getClosestPlayerAndBodyPartToCursor() local camera=workspace.CurrentCamera;local viewportSize=camera.ViewportSize;local cursorPos=Vector2.new(viewportSize.X/2 ,viewportSize.Y/2 );local closestDistance=math.huge;local closestPlayer=nil;local closestBodyPart=nil;for _,player in ipairs(game.Players:GetPlayers()) do if ((player~=game.Players.LocalPlayer) and player.Character and player.Character:FindFirstChild("Humanoid") and (player.Character.Humanoid.Health>0)) then local isTeammate=player.Team==game.Players.LocalPlayer.Team ;if ( not isTeammate or lockOnEveryone) then local bodyParts={player.Character:FindFirstChild("HumanoidRootPart"),player.Character:FindFirstChild("Torso"),player.Character:FindFirstChild("UpperTorso")};for _,part in ipairs(bodyParts) do if part then local partPos=part.Position;local screenPoint,onScreen=camera:WorldToViewportPoint(partPos);if onScreen then local distanceToCursor=(cursorPos-Vector2.new(screenPoint.X,screenPoint.Y)).Magnitude;if ((distanceToCursor<aimFOV) and (distanceToCursor<closestDistance)) then closestDistance=distanceToCursor;closestPlayer=player;closestBodyPart=part;end end end end end end end return closestPlayer,closestBodyPart;end local function lockOntoPlayer(player,bodyPart) if (player and bodyPart) then local camera=workspace.CurrentCamera;local targetVelocity=bodyPart.Velocity;local targetPosition=bodyPart.Position;local distanceToTarget=(camera.CFrame.Position-targetPosition).Magnitude;local predX=(predictionX * distanceToTarget)/80 ;local predY=(predictionY * distanceToTarget)/100 ;local predictedPosition=targetPosition + Vector3.new(targetVelocity.X * predX ,targetVelocity.Y * predY ,targetVelocity.Z * predX ) ;local currentCFrame=camera.CFrame;local targetCFrame=CFrame.new(currentCFrame.Position,predictedPosition);local lerpAmount=1 -(smoothness/10) ;camera.CFrame=currentCFrame:Lerp(targetCFrame,lerpAmount);end end local AimbotTab=Window:MakeTab({Name="Aimbot",Icon="rbxassetid://4483345998",PremiumOnly=false});local AimbotSection=AimbotTab:AddSection({Name="Main Controls"});AimbotTab:AddToggle({Name="Enable Aimbot",Default=false,Callback=function(Value) aimbotEnabled=Value;targetingPlayer=nil;targetingBodyPart=nil;end});AimbotTab:AddToggle({Name="Lock onto Everyone (Including Teammates)",Default=false,Callback=function(Value) lockOnEveryone=Value;end});AimbotTab:AddSlider({Name="Prediction Time (X-axis)",Min=0.05,Max=1,Default=0.05,Increment=0.01,Callback=function(Value) predictionX=Value;end});AimbotTab:AddSlider({Name="Prediction Time (Y-axis)",Min=0.05,Max=1,Default=0.05,Increment=0.01,Callback=function(Value) predictionY=Value;end});AimbotTab:AddSlider({Name="Smoothness",Min=1,Max=10,Default=10,Increment=1,Callback=function(Value) smoothness=Value;end});local VisualsTab=Window:MakeTab({Name="Visuals",Icon="rbxassetid://6031763426",PremiumOnly=false});local VisualsSection=VisualsTab:AddSection({Name="FOV Settings"});VisualsSection:AddToggle({Name="Enable FOV Circle",Default=false,Callback=function(Value) fovCircle.Visible=Value;end});VisualsSection:AddSlider({Name="FOV Circle Size",Min=50,Max=500,Default=150,Increment=5,Callback=function(Value) aimFOV=Value;fovCircle.Radius=aimFOV;end});local PremiumTab=Window:MakeTab({Name="Premium",Icon="rbxassetid://6031068423",PremiumOnly=false});local PremiumSection=PremiumTab:AddSection({Name="Premium Features"});PremiumTab:AddButton({Name="Buy Premium [Coming Soon.]",Callback=function() game:GetService("StarterGui"):SetCore("SendNotification",{Title="Discord",Text="Link successfully copied.",Icon="rbxassetid://6035056475",Duration=5});setclipboard("https://discord.gg/J2WtcE22vN");end});local MiscTab=Window:MakeTab({Name="Misc",Icon="rbxassetid://6031280882",PremiumOnly=false});local MiscSection=MiscTab:AddSection({Name="Settings"});MiscSection:AddToggle({Name="Enable Anti-Lock",Default=false,Callback=function(Value) getgenv().Sky=Value;end});timerLabel=MiscSection:AddLabel("Injection Time: 00:00");local userInputService=game:GetService("UserInputService");userInputService.InputBegan:Connect(function(input,gameProcessed) if ( not gameProcessed and aimbotEnabled and (input.UserInputType==Enum.UserInputType.MouseButton2)) then holdingRMB=true;targetingPlayer,targetingBodyPart=getClosestPlayerAndBodyPartToCursor();end end);userInputService.InputEnded:Connect(function(input,gameProcessed) if ( not gameProcessed and (input.UserInputType==Enum.UserInputType.MouseButton2)) then holdingRMB=false;targetingPlayer=nil;targetingBodyPart=nil;end end);game:GetService("RunService").RenderStepped:Connect(function() if (aimbotEnabled and holdingRMB and targetingPlayer and targetingBodyPart) then lockOntoPlayer(targetingPlayer,targetingBodyPart);end end);OrionLib:Init(); end
